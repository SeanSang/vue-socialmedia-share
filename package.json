{
  "name": "vue-socialmedia-share",
  "version": "0.3.0",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle":
      "vue-cli-service build --target lib --name vue-socialmedia-share ./src/components/index.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.5.16",
    "vue-awesome": "^3.0.0"
  },
  "main": "./dist/vue-socialmedia-share.common.js",
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.12",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.12",
    "@vue/cli-service": "^3.0.0-beta.12",
    "vue-template-compiler": "^2.5.16"
  },
  "babel": {
    "presets": ["@vue/app"]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": ["plugin:vue/essential", "eslint:recommended"],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
